    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.horarios.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.horarios.ActividadHorario"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.horarios.ActividadGenerarQr"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.horarios.ActividadBorrarAsignatura"
            android:label="@string/app_name" >
        </activity>
    </application>

</manifest>

